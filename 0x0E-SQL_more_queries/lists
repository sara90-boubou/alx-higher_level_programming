ch record should display: tv_shows.title - tv_show_genres.genre_id
   -- Results must be sorted in ascending order by tv_shows.title
   -- and tv_show_genres.genre_id
   -- The database name will be passed as an argument of the mysql command

SELECT title, tv_show_genres.genre_id FROM tv_shows
LEFTch record should display:
      -- tv_shows.title
   -- Results must be sorted in ascending order by the show title
   -- The database name will be passed as an argument of the mysql command

SELECT tv_shows.title FROM tv_shows
WHERE tv_shows.id NOT IN (
      SELECT tv_shows.id FROM tv_shows
      JOIN tv_show_genres ON tv_shows.id=tv_show_genres.show_id
      JOIN tv_genres ON tv_genres.id=tv_show_genres.genre_id
      WHERE tv_genres.name = "Comedy" )
ORDER BY tv_shows.title; JOIN tv_show_genres ON id=tv_show_genres.show_id
WHERE tv_show_genres.show_id IS NULL
ORDER BY title, tv_show_genres.genre_id;
